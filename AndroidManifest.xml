<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.malikumair.mobileattendance">

    <application
        android:allowBackup="true"
        android:icon="@drawable/materialicons"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".clickedClassOperations"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity android:name=".DoAttendance" />
        <activity android:name=".addNewClass" />
        <activity android:name=".studentsOverallAttendance" />
        <activity android:name=".particularStudentsAttendance" />
        <activity android:name=".checkAttendanceDoneStatus" />
    </application>

</manifest>